{
  "name": "my-project",
  "version": "1.0.0",
  "scripts": {
    "start": "vite --host ",
    "pre-build": "tsc",
    "build:dev": "tsc && vite build && npm run copy-files",
    "build:production": "npm run build:dev && npm run pack-gltf",
    "serve": "vite preview",
    "copy-files": "copy-files-from-to",
    "pack-gltf": "npm run pack-gltf --prefix node_modules/@needle-tools/engine",
    "heroku-postbuild": "npm run build:production"
  },
  "dependencies": {
    "@needle-tools/engine": "file:../../Library/PackageCache/com.needle.engine@2.67.8-pre.1/package~",
    "axios": "^1.3.4",
    "jsonwebtoken": "^9.0.0",
    "three": "file:../../Library/PackageCache/com.needle.engine@2.67.8-pre.1/package~/node_modules/three"
  },
  "devDependencies": {
    "@needle-tools/needle-component-compiler": "^1.0.0",
    "@types/express": "^4.17.17",
    "@types/three": "0.146.0",
    "@vitejs/plugin-basic-ssl": "^0.1.2",
    "autoprefixer": "^10.4.14",
    "copy-files-from-to": "^3.3.0",
    "express": "^4.18.2",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.3.0",
    "typescript": "^4.5.5",
    "vite": "^3.1.4",
    "vite-plugin-compression": "^0.5.1"
  },
  "copyFiles": [
    {
      "from": "assets/**/*.*",
      "to": "dist/assets/"
    },
    {
      "from": "node_modules/@needle-tools/engine/src/include/**/*.*",
      "to": "dist/include/"
    },
    {
      "from": "node_modules/@needle-tools/engine/src/include/**/*.*",
      "to": "./include/"
    },
    {
      "from": "include/**/*.*",
      "to": "dist/include/"
    }
  ]
}
